create database students;
create table details
(
    name char(20),
    roll_no varchar(20),
    class char(20),
    year integer(20)
    );
    show databases;
desc details;
insert into details values('ajith','15bec209',"ece","2018");
insert into details values('dhoni','15bec211',"ece","2018");
insert into details values('raina','15bec219',"eee","2018");
insert into details values('watson','15bec200',"eie","2018");
select * from details where class='ece';
    
